{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "pageTitle": {
      "type": "string",
      "required": true,
      "unique": true,
      "default": "Tjenester"
    },
    "slug": {
      "type": "uid",
      "targetField": "pageTitle",
      "required": true
    },
    "mediaModule": {
      "type": "component",
      "repeatable": false,
      "component": "modules.full-media-module"
    },
    "serviceHeadingComponent": {
      "type": "component",
      "repeatable": false,
      "component": "modules.service-heading-content",
      "required": true
    },
    "dynamicServicePageContent": {
      "type": "dynamiczone",
      "components": [
        "modules.txt-l-landscape-r-module",
        "modules.txt-l-square-r-module",
        "modules.txt-r-landscape-l-module",
        "modules.txt-r-square-r-module",
        "shared.seo",
        "modules.quote-module",
        "modules.rich-text-module",
        "shared.link",
        "modules.shortcuts-module"
      ],
      "required": true
    }
  }
}
