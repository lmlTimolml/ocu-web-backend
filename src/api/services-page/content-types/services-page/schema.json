{
  "kind": "singleType",
  "collectionName": "services_pages",
  "info": {
    "singularName": "services-page",
    "pluralName": "services-pages",
    "displayName": "ServicesPage",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "pageTitle": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "pageTitle",
      "required": true
    },
    "heroSection": {
      "type": "component",
      "repeatable": false,
      "component": "layout.hero",
      "required": true
    },
    "breadcrumbpath": {
      "type": "component",
      "repeatable": false,
      "component": "layout.breadcrumb-path"
    },
    "serviceLinks": {
      "type": "component",
      "repeatable": true,
      "component": "shared.link",
      "max": 4
    },
    "servicesPopulate": {
      "displayName": "servicesPopulate",
      "type": "component",
      "repeatable": false,
      "component": "layout.services-populate",
      "required": true
    }
  }
}
